<div class="modal fade" id="confirmModal" data-backdrop="static" tabindex="-1"  role="dialog">
	<div class="modal-dialog modal-dialog-centered">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="confirmTitle">Are you sure?</h4>
				<button type="button" id="confirmCancelBtn" class="close" data-dismiss="modal">&times;</button>
			</div>
			<div class="modal-body">
				<p id="confirmMessage"></p>
			</div>
			<div class="modal-footer">
				<button type="button" id="confirmActionBtn" class="btn btn-default">Yes</button>
				<button type="button" id="confirmNoBtn" class="btn btn-default" data-dismiss="modal">No</button>
			</div>
		</div>
	</div>
</div>
<div class="modal fade" id="popupModal" data-backdrop="static" tabindex="-1"  role="dialog">
	<div class="modal-dialog modal-dialog-centered">
		<!-- Modal content-->
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="popupTitle"></h4>
				<button type="button" class="close" data-dismiss="modal">&times;</button>
			</div>
			<div class="modal-body">
				<p id="popupMessage"></p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>